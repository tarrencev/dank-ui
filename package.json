{
  "name": "dank-ui",
  "version": "0.2.0",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib/",
    "dev": "run-when-changed --watch 'src/**/*.js'  --watch node_modules/**/*.js --exec 'npm run prepare'",
    "build": "babel src/ --out-dir lib/",
    "prepare": "npm run prepublish",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-watch": "^2.0.7",
    "run-when-changed": "^2.1.0"
  },
  "dependencies": {
    "bignumber.js": "^6.0.0",
    "draft-js": "^0.10.5",
    "draft-js-export-markdown": "^1.2.0",
    "draft-js-inline-toolbar-plugin": "^3.0.0",
    "draft-js-markdown-shortcuts-plugin": "^0.5.0",
    "draft-js-plugins-editor": "^2.1.1",
    "draft-js-static-toolbar-plugin": "^3.0.0",
    "markdown-draft-js": "^1.1.6",
    "react-onclickoutside": "^6.7.1",
    "react-tabs": "^2.3.0",
    "recompose": "^0.26.0",
    "smooth-ui": "^4.3.0"
  }
}
